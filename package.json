{
    "name": "vsop-language-support",
    "displayName": "VSOP Language Support",
    "description": "Language support for vsop files",
    "version": "1.0.1",
    "publisher": "DavidBirtles",
    "repository": {
        "url": "https://github.com/pydawan82/vsop-language-support"
    },
    "icon": "icon.png",
    "author": {
        "name": "David Birtles"
    },
    "engines": {
        "vscode": "^1.61.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "vsop",
                "extensions": [
                    ".vsop"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "vsop",
                "scopeName": "source.vsop",
                "path": "./syntaxes/vsop.tmLanguage.json"
            }
        ]
    }
}